tail-server-log
create-pool

exec restic init --option rclone.program=restic-ceph-server --repo rclone: --password-file $WORK/password.txt

exec restic backup --json $WORK --option rclone.program=restic-ceph-server --repo rclone: --password-file $WORK/password.txt
stdin stdout
exec jq -s '[.[] | select(.message_type == "summary")] | .[0] | {files_new: .files_new, files_changed: .files_changed, files_unmodified: .files_unmodified, dirs_new: .dirs_new, dirs_changed: .dirs_changed, dirs_unmodified: .dirs_unmodified, snapshot_id: .snapshot_id}'
stdin stdout
scrubhex stdout stdout
cmp stdout out.json

rados-object-count snapshots/
cmp stdout snapshots.txt

-- password.txt --
secret
-- snapshots.txt --
1
-- out.json --
{
  "files_new": 3,
  "files_changed": 0,
  "files_unmodified": 0,
  "dirs_new": 4,
  "dirs_changed": 0,
  "dirs_unmodified": 0,
  "snapshot_id": "[HEX]"
}

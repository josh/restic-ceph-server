tail-server-log
create-pool

exec restic-ceph-server --listen $WORK/restic.sock &server&
wait4socket $WORK/restic.sock

exec curl --silent --unix-socket $WORK/restic.sock --request POST 'http://localhost/?create=true'

exec curl --silent --write-out '%{http_code}' --unix-socket $WORK/restic.sock --request POST --header 'Content-Type: application/octet-stream' --data-binary @config-data http://localhost/config
stdout '200'

exec curl --silent --write-out '%{http_code}' --unix-socket $WORK/restic.sock --request DELETE http://localhost/config
stdout '200'

exec curl --silent --write-out '%{http_code}' --unix-socket $WORK/restic.sock --request GET http://localhost/config
stdout '404'

exec curl --silent --write-out '%{http_code}' --unix-socket $WORK/restic.sock --request DELETE http://localhost/config
stdout '200'

exec curl --silent --write-out '%{http_code}' --unix-socket $WORK/restic.sock --request POST --header 'Content-Type: application/octet-stream' --data-binary @config-data http://localhost/config
stdout '200'

exec curl --silent --unix-socket $WORK/restic.sock --request GET http://localhost/config
cmp stdout config-data

kill server

